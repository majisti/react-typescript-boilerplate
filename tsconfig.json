{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es7",
      "dom"
    ],
    "baseUrl": ".",
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": false,
    "paths": {
      "*": [
        "src/*"
      ]
    }
  }
}